'.text.tex.latex':
  'SI':
    'prefix': 'si'
    'body': '\\\\SI{${1:number}}{${2:unit}}'
  'grados celsius':
    'prefix': 'celsius'
    'body': '\\\\SI{${1:number}}{\\\\celsius}'
  'gramos':
    'prefix': 'gramos'
    'body': '\\\\SI{${1:number}}{\\\\gram}'
  'xsim exercise + solution':
    'prefix': 'xsim'
    'body': """
      \\\\begin{exercise}
        ${1:content}
      \\\\end{exercise}
      \\\\begin{solution}
        ${2:solution}
      \\\\end{solution}
    """
